{% block main %}
    <!-- Breadcrumbs -->
    <ul class="breadcrumbs" itemscope="" itemtype="http://schema.org/BreadcrumbList">
        {% if is_mobile() or is_tablet() %}
            <li class="active-breadcrumbs" itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem">
                <a href="{{ path("app_insurancecompany_feedback_index") }}" class="" itemprop="item">Добавление отзыва
                    <meta itemprop="name" content="Добавление отзыва">
                </a>
                <meta itemprop="position" content="1">
            </li>
        {% else %}
            <li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem">
                <a href="/" itemprop="item">Главная
                    <meta itemprop="name" content="Главная">
                </a>
                <meta itemprop="position" content="1">
            </li>
            <li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem">
                <a href="{{ path("app_insurancecompany_feedback_index") }}" itemprop="item">Отзывы
                    <meta itemprop="name" content="Отзывы">
                </a>
                <meta itemprop="position" content="2">
            </li>
            <li class="active-breadcrumbs" itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem">
                <a itemprop="item">Добавление отзыва
                    <meta itemprop="name" content="Добавление отзыва">
                </a>
                <meta itemprop="position" content="3">
            </li>
        {% endif %}
    </ul>

    <!-- Pages Title -->
    {#            {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}#}
    <h1 class="page-title">Добавить отзыв о компании</h1>

    <!-- Wrap -->
{#    {{ form_start(form, { attr: 'action': {{ url('reviews_add', {}, false) }}, 'method': 'POST', id: "form-comments"} }) }}#}
    {{ form_start(form, {'action': path('app_insurancecompany_feedback_new'), 'method': 'POST', 'attr': {
        id: 'form-comments'
    }}) }}
    <div class="white_block">

    <div class="feedback__top">
        <div class="add_reviews-select">
            <label class="input__wrap_label" for="user_pass">Выбор региона: </label>
            {{ form_row(form.region) }}
        </div>
        <div class="add_reviews-select">
            <label class="input__wrap_label" for="user_pass">Список страховых компаний : </label>
            {{ form_row(form.branch) }}
        </div>
    </div>

    <h3 class="feedback__title">
        Расскажите о вашей ситуации
    </h3>
    {% if not is_granted('IS_AUTHENTICATED_REMEMBERED') %}
    <div class="input__wrap name_user_no_Authorized">
        <label for="name_user" class="input__wrap_label">Введите свое имя</label>
