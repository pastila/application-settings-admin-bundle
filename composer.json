{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:accurateweb/email-template-bundle.git"
        },
        {
        "type": "vcs",
        "url": "git@github.com:accurateweb/client-application-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/accurateweb/image-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/accurateweb/media-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/accurateweb/application-settings-admin-bundle.git"
        },
        {
          "type": "vcs",
          "url": "https://github.com/accurateweb/location-bundle.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php",
            "web/kdteam-classes/Mobile_Detect.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "accurateweb/application-settings-admin-bundle": "dev-development",
        "accurateweb/client-application-bundle": "dev-master",
        "accurateweb/email-template-bundle": "dev-development",
        "accurateweb/image-bundle": "2.x-dev",
        "accurateweb/location-bundle": "dev-master",
        "accurateweb/media-bundle": "2.x-dev",
        "deployer/deployer": "^6.8",
        "doctrine/doctrine-bundle": "^1.8.1",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/user-bundle": "~2.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "phpunit/phpunit": "^5.7",
        "pixassociates/sortable-behavior-bundle": "^1.5",
        "redcode/tree-bundle": "^0.1.0@alpha",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "sonata-project/admin-bundle": "^3.4",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "stfalcon/tinymce-bundle": "^2.5",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "deployer/recipes": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpstan/phpstan": "^0.12.52",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
