# Set default values for all following accounts.
# smtp account configuration
defaults
account        default
logfile        /var/log/msmtp.log
host smtp.yandex.ru
port 587
from no-reply@bezbahil.ru
keepbcc on
auth on
user no-reply@bezbahil.ru
password 123123no-reply123123
tls on
tls_certcheck off
tls_starttls on
